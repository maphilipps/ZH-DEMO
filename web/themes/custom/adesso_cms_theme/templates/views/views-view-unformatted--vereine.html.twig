{#
/**
 * @file
 * Theme override for unformatted rows in Vereine view.
 * Modern list layout with dividers between items.
 */
#}
{% if title %}
  <h3>{{ title }}</h3>
{% endif %}

{# Modern list layout with dividers #}
<ul role="list" class="divide-y divide-gray-100">
  {% for row in rows %}
    {{ row.content }}
  {% endfor %}
</ul>