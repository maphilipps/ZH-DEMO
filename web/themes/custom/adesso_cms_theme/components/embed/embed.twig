<div class="py-4 px-4 mx-auto max-w-screen-xl lg:py-16 border bg-card text-card-foreground border-none shadow-none">

  {% if pre_headline %}
    <h3 class="mb-4 font-bold text-2xl tracking-tight text-center leading-none">
      {{ pre_headline }}
    </h3>
  {% endif %}
  {% if title %}
    <h2 class="text-3xl font-bold text-center mb-6 md:mb-8">{{ title }}</h2>
  {% endif %}

  <div>
    <div class="prose max-w-none [&_iframe]:w-full [&_iframe]:max-w-full">
      {{ embed }}
    </div>
  </div>
</div>
