{#
/**
 * @file
 * Link list module template - simplified placeholder.
 *
 * Variables:
 * - items: Array of link items
 */
#}
{% if items %}
  <div class="link-list-module">
    <div class="container mx-auto px-6 lg:px-8">
      <ul class="link-list">
        {% for item in items %}
          <li class="link-list__item">
            {{ item }}
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>
{% endif %}