{#
/**
 * Field "Tipps" for content type "Praxisbeispiel"
 * To be displayed in a bullet-points module
 *
 * @file
 * Theme override for a field.
 */
#}

{% set array = [] %}
{% for item in items %}
  {% set array = array|merge([{text: item.content['#text']|striptags}]) %}
{% endfor %}

{{ include('@module/bullet-points.html.twig', {
  listType: 'number',
  headline: '',
  items: array,
}) }}
