{# /** * @file * Pricing card paragraph template (child component - no wrapper needed). */ #} {{ title_prefix }}
{{ title_suffix }}
{% embed '@adesso_cms_theme/pricing/pricing-card.twig' with {
  "title": content.field_title|render|striptags,
  "pre_headline": content.field_eyebrow|render|striptags,
  "features": content.field_features_text|render|striptags|striptags|trim|split('\n'),
} only %}
  {% block primary_action %}
    {% if content.field_link[0]['#url'] %}
      {{ content.field_link }}
    {% endif %}
  {% endblock %}
{% endembed %}
